<section class="row p-5 bg-light">
  <div class="col-12">
    <h1 class="text-center">Projetos</h1>
  </div>

  <% projects.forEach(project => { %>

  <div class="col-md-4">
    <div class="card">
      <div class="card-body">
        <h5 class="card-title"><%= project.nome %></h5>
        <div class="card-text">
          
          <%= project.descricao %> 
          <br />
          <br />
          <h5>Tecnologia:</h5>
          <p><%= project.tecnologia %></p>

        </div>
      </div>
    </div>
  </div>

  <% }); %>
</section>
